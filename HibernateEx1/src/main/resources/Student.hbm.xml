<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hcl.entities">
 <class name="org.hcl.entities.Student" table="STUDENT">
  <id name="id" type="int">
   <column name="SID"/>
   <generator class="assigned"/>
  </id>
  <property column="NAME" generated="never" lazy="false" name="name"/>
  <property column="ADDRESS" generated="never" lazy="false" name="address"/>
 </class>
</hibernate-mapping>
